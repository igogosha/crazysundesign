<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Crazysundesign.com!{% endblock %}</title>
        {% block stylesheets %}

            <meta http-equiv="content-type" content="text/html;charset=UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

            <!-- BEGIN PLUGIN CSS -->
            {#<link href="{{ asset("bundles/crazysunmain/") }}plugins/pace/pace-theme-flash.css" rel="stylesheet" type="text/css" media="screen">#}
            {#<link href="{{ asset("bundles/crazysunmain/") }}plugins/jquery-slider/css/jquery.sidr.light.css" rel="stylesheet" type="text/css" media="screen">#}
            <!-- END PLUGIN CSS -->

            <!-- BEGIN CORE CSS FRAMEWORK -->
            <link href="{{ asset("bundles/crazysunmain/") }}plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
            <link href="{{ asset("bundles/crazysunmain/") }}plugins/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet" type="text/css">
            <link href="{{ asset("bundles/crazysunmain/") }}plugins/font-awesome/css/font-awesome.css" rel="stylesheet" type="text/css">
            <link href="{{ asset("bundles/crazysunmain/") }}css/animate.min.css" rel="stylesheet" type="text/css">
            <!-- END CORE CSS FRAMEWORK -->

            <!-- BEGIN CSS TEMPLATE -->
            <link href="{{ asset("bundles/crazysunmain/") }}css/style.css" rel="stylesheet" type="text/css">
            <link href="{{ asset("bundles/crazysunmain/") }}css/responsive.css" rel="stylesheet" type="text/css">
            <link href="{{ asset("bundles/crazysunmain/") }}css/custom-icon-set.css" rel="stylesheet" type="text/css">
            <!-- END CSS TEMPLATE -->

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="error-body no-top">

        <div class="container">
            <div class="clearfix"></div>
            {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <!-- BEGIN CORE JS FRAMEWORK-->
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-1.8.3.min.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-ui/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/boostrapv3/js/bootstrap.min.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/breakpoints.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
            <!-- END CORE JS FRAMEWORK -->
            <!-- BEGIN PAGE LEVEL JS -->
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/pace/pace.min.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-block-ui/jqueryblockui.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-slider/jquery.sidr.min.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-numberAnimate/jquery.animateNumbers.js" type="text/javascript"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
            <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
            <script src="{{ asset("bundles/crazysunmain/") }}plugins/jquery-gmap/gmaps.js" type="text/javascript"></script>
            <!-- END PAGE LEVEL PLUGINS -->
            <!-- PAGE JS -->
            <script src="{{ asset("bundles/crazysunmain/") }}js/google_maps.js" type="text/javascript"></script>
            <!-- BEGIN CORE TEMPLATE JS -->
            <script src="{{ asset("bundles/crazysunmain/") }}js/core.js" type="text/javascript"></script>
            {#<script src="{{ asset("bundles/crazysunmain/") }}js/chat.js" type="text/javascript"></script>#}
            {#<script src="{{ asset("bundles/crazysunmain/") }}js/demo.js" type="text/javascript"></script>#}
            <!-- END CORE TEMPLATE JS -->

        {% endblock %}
    </body>
</html>
